    "@livekit/components-styles": "^1.2.0",
    "@react-three/drei": "^9.120.3",
    "@react-three/fiber": "^8.17.10",
    "@react-three/rapier": "^1.5.0",
    "@stripe/stripe-js": "^3.3.0",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "@vercel/node": "^3.2.29",
    "cannon-es": "^0.20.0",
    "clsx": "^2.1.1",
